const numbers = [13, 5, 7, 200, 3, 7.9]

func bubbleSort(arr) {

  const n = length(arr)

  for (var i = 0, i < n - 1, i += 1) {

    for (var j = 0, j < n - i - 1, j += 1) {

      if (arr[j] > arr[j + 1]) {

        # Swap the elements
        const temp = arr[j]
        arr[j] = arr[j + 1]
        arr[j + 1] = temp
      }
    }
  }
  print(arr)
}

bubbleSort(numbers)